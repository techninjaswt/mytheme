{
  "sections": {
    "main": {
      "type": "main-article",
      "blocks": {
        "title": {
          "type": "article-header",
          "settings": {
            "full_width": true,
            "blog_show_tags": true,
            "blog_show_excerpt": true,
            "blog_show_author": true,
            "blog_show_date": true,
            "image_height": "medium"
          }
        },
        "content": {
          "type": "content",
          "settings": {
            "show_share": true,
            "share_label": "Share:",
            "show_share_facebook": true,
            "show_share_twitter": true,
            "show_share_pinterest": true,
            "show_share_copy": true
          }
        },
        "article_footer": {
          "type": "article-footer",
          "settings": {
            "blog_show_author": true,
            "blog_show_tags": true,
            "blog_show_date": true
          }
        }
      },
      "block_order": [
        "title",
        "content",
        "article_footer"
      ],
      "custom_css": [
        "@media (min-width: 768px) {.article-template__header--overlay.article-template__header--large .article-template__wrapper {min-height: 50rem; }}"
      ],
      "settings": {
        "header_variant": "overlay",
        "color_scheme": "",
        "opacity_overlay": 0.3,
        "padding_top": "no-indent",
        "padding_bottom": "m"
      }
    },
    "featured_blog": {
      "type": "featured-blog",
      "settings": {
        "subheading": "",
        "heading": "Latest Articles",
        "heading_size": "h2",
        "post_layout": "column",
        "blog": "news",
        "post_limit": 6,
        "post_limit_grid": 4,
        "show_image": true,
        "show_tags": true,
        "show_date": true,
        "show_author": true,
        "show_excerpt": false,
        "show_view_all": false,
        "button_label": "Button label",
        "button_style": "simple",
        "padding_top": "m",
        "padding_bottom": "m"
      }
    }
  },
  "order": [
    "main",
    "featured_blog"
  ]
}

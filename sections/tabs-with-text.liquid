        },
        {
          "value": "m",
          "label": "t:sections.all.section-padding.options__4.label"
        },
        {
          "value": "l",
          "label": "t:sections.all.section-padding.options__5.label"
        },
        {
          "value": "xl",
          "label": "t:sections.all.section-padding.options__6.label"
        }
      ],
      "default": "m",
      "label": "t:sections.all.section-padding.padding_bottom.label"
    }
  ],
  "blocks": [
    {
      "type": "text_tab",
      "name": "t:sections.tabs-with-text.blocks.text_tab.name",
      "settings": [
        {
          "type": "text",
          "id": "tab_heading",
          "default": "Tab heading",
          "label": "t:sections.all.heading.label"
        },
        {
          "type": "image_picker",
          "id": "image",
          "label": "t:sections.tabs-with-text.blocks.text_tab.settings.image.label",
          "info": "t:sections.tabs-with-text.blocks.text_tab.settings.image.info"
        },
        {
          "type": "image_picker",
          "id": "image_2",
          "label": "t:sections.tabs-with-text.blocks.text_tab.settings.image_2.label"
        },
        {
          "type": "richtext",
          "id": "text",
          "default": "<p>Some description</p>",
          "label": "t:sections.all.text.label"
        },
        {
          "type": "header",
          "content": "t:sections.all.button.header_button.content"
        },
        {
          "type": "checkbox",
          "id": "show_button",
          "default": false,
          "label": "t:sections.all.button.button_show.label"
        },
        {
          "type": "text",
          "id": "button_label",
          "default": "Button Label",
          "label": "t:sections.all.button.button_label.label"
        },
        {
          "type": "url",
          "id": "button_link",
          "label": "t:sections.all.button.button_link.label"
        },
        {
          "type": "select",
          "id": "button_style",
          "options": [
            {
              "value": "primary",
              "label": "t:sections.all.button.button_style.options__1.label"
            },
            {
              "value": "secondary",
              "label": "t:sections.all.button.button_style.options__2.label"
            },
            {
              "value": "tertiary",
              "label": "t:sections.all.button.button_style.options__3.label"
            },
            {
              "value": "simple",
              "label": "t:sections.all.button.button_style.options__4.label"
            }
          ],
          "default": "simple",
          "label": "t:sections.all.button.button_style.label"
        }
      ]
    },
    {
      "type": "characteristics_tab",
      "name": "t:sections.tabs-with-text.blocks.chars_tab.name",
      "settings": [
        {
          "type": "text",
          "id": "tab_heading",
          "default": "Tab heading",
          "label": "t:sections.all.heading.label"
        },
        {
          "type": "range",
          "id": "columns",
          "min": 1,
          "max": 4,
          "step": 1,
          "default": 3,
          "label": "t:sections.tabs-with-text.blocks.chars_tab.settings.columns.label"
        }
      ]
    },
    {
      "type": "column_tab",
      "name": "t:sections.tabs-with-text.blocks.column_tab.name",
      "settings": [
        {
          "type": "text",
          "id": "tab_heading",
          "default": "Tab heading",
          "label": "t:sections.all.heading.label"
        },
        {
          "type": "range",
          "id": "columns_in_row",
          "min": 1,
          "max": 4,
          "step": 1,
          "default": 4,
          "label": "t:sections.tabs-with-text.blocks.column_tab.settings.columns_in_row.label"
        },
        {
          "type": "color_scheme",
          "id": "color_scheme",
          "default": "background-2",
          "label": "t:sections.all.color_scheme.label"
        },
        {
          "type": "header",
          "content": "t:sections.tabs-with-text.blocks.column_tab.settings.column_header_1.content"
        },
        {
          "type": "checkbox",
          "id": "show_column_1",
          "default": true,
          "label": "t:sections.tabs-with-text.blocks.column_tab.settings.show_column.label"
        },
        {
          "type": "select",
          "id": "icon_1",
          "options": [
            {
              "value": "none",
              "label": "t:sections.all.icon_pack.options__1.label"
            },
            {
              "value": "alert",
              "label": "t:sections.all.icon_pack.options__2.label"
            },
            {
              "value": "bag",
              "label": "t:sections.all.icon_pack.options__3.label"
            },

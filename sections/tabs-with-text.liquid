{{ 'section-tabs-with-text.css' | asset_url | stylesheet_tag }}
<script src="{{ 'tabs-with-text.js' | asset_url }}" defer="defer"></script>

{%- style -%}
  {% render 'section-margin' %}
  {% render 'section-padding' %}
{%- endstyle -%}

<div class="tabs-with-text section-{{ section.id }} section-{{ section.id }}-padding twt">
  <div class="container">
    <div class="titles">
      {%- if section.settings.subheading != blank -%}
        <div class="twt__subheading subtitle">
          {{ section.settings.subheading }}
        </div>
      {%- endif -%}

      {%- if section.settings.heading != blank -%}
        <h2 class="twt__heading heading {{ section.settings.heading_size }}">
          {{ section.settings.heading }}
        </h2>
      {%- endif -%}
    </div>

    <div class="twt__tabs swiper no-js-hidden">
      <div class="swiper-wrapper">
        {%- for block in section.blocks -%}
          {%- if block.settings.tab_heading != blank -%}
            <h3
              class="twt__tabs-item swiper-slide link-hover-line {{ section.settings.tab_heading_size }}"
              {{ block.shopify_attributes }}
              {% if forloop.index == 1 %}
                data-active
              {% endif %}
            >
              {{ block.settings.tab_heading | escape }}
            </h3>
          {%- endif -%}
        {% endfor %}
      </div>

      <div class="twt__nav">
        <div class="twt__prev focus-inset">
          {% render 'icon-button-arrow' %}
        </div>
        <div class="twt__next focus-inset">
          {% render 'icon-button-arrow' %}
        </div>
      </div>
    </div>
    <div class="twt__content">
      {%- for block in section.blocks -%}
        <h3
          class="twt__tabs-item swiper-slide link-hover-line {{ section.settings.tab_heading_size }} js-hidden"
          {{ block.shopify_attributes }}
            data-active
        >
          {{ block.settings.tab_heading | escape }}
        </h3>
        <div
          class="twt__content-item no-js-flex"
          {% if forloop.index == 1 %}
            style="display: flex;"
          {% endif %}
        >
          {%- case block.type -%}
            {%- when 'text_tab' -%}
              {%- if block.settings.text != blank or block.settings.image != blank or block.settings.show_button -%}
                <div class="twt__text" {{ block.shopify_attributes }}>
                  <div class="twt__image-wrapper">
                    {% if block.settings.image != blank %}
                      <img
                        srcset="{{ block.settings.image | image_url: width: 100 }} 1x, {{ block.settings.image | image_url: width: 200 }} 2x"
                        src="{{ block.settings.image | image_url: width: 100 }}"
                        width="100"
                        height="auto"
                        loading="lazy"
                        class="twt__image"
                      >
                      {% if block.settings.image_2 != blank %}
                        <img
                          srcset="{{ block.settings.image_2 | image_url: width: 100 }} 1x, {{ block.settings.image_2 | image_url: width: 200 }} 2x"
                          src="{{ block.settings.image_2 | image_url: width: 100 }}"
                          width="100"
                          height="auto"
                          loading="lazy"
                          class="twt__image twt__image--dark"
                        >
                      {% endif %}
                    {% elsif block.settings.image_2 != blank %}
                      <img
                        srcset="{{ block.settings.image_2 | image_url: width: 100 }} 1x, {{ block.settings.image_2 | image_url: width: 200 }} 2x"
                        src="{{ block.settings.image_2 | image_url: width: 100 }}"
                        width="100"
                        height="auto"
                        loading="lazy"
                        class="twt__image twt__image--dark"
                      >
                    {% endif %}
                  </div>
                  {{ block.settings.text }}
                  {%- if block.settings.show_button -%}
                    <a
                      {% if block.settings.button_link != blank %}
                        href="{{ block.settings.button_link }}"
                      {% endif %}
                      class="button twt__button button--{{ block.settings.button_style }} {% if block.settings.button_style == 'simple' %}button--arrow{% endif %} focus-inset"
                      {% if block.settings.button_link == blank %}
                        aria-disabled="true"
                      {% endif %}
                    >
                      <span>
                        {{- block.settings.button_label | escape -}}
                      </span>
                      {% if block.settings.button_style == 'simple' %}
                        {% render 'icon-button-arrow' %}
                      {% endif %}
                    </a>
                  {%- endif -%}
                </div>
              {%- endif -%}
            {%- when 'characteristics_tab' -%}
              {%- if product.metafields.characteristics.name.value.size > 0 -%}
                <div
                  class="twt__characteristics"
                  style="--columns: {{ block.settings.columns }};"
                  {{ block.shopify_attributes }}
                >
                  {%- for item in product.metafields.characteristics.name.value -%}
                    <div class="twt__row">
                      <div class="twt__row-title">{{ item }}</div>
                      <div class="twt__row-descr">
                        {{ product.metafields.characteristics.description.value[forloop.index0] }}
                      </div>
                    </div>
                  {% endfor %}
                </div>
              {% endif %}
            {%- when 'column_tab' -%}
              <div
                class="multicolumn-list__wrapper multicolumn-list__wrapper--{{ block.settings.columns_in_row }} multicolumn-list__wrapper--static twt__multicolumn"
                {{ block.shopify_attributes }}
              >
                {% if block.settings.show_column_1 %}
                  <div class="multicolumn-card multicolumn-list__item color-{{ block.settings.color_scheme }} background">
                    {%- if block.settings.icon_1 != 'none' -%}
                      <div class="multicolumn-card__image-wrapper multicolumn-card__image-wrapper--icon multicolumn-card__image-wrapper--medium">
                        {% render 'icon-pack', icon: block.settings.icon_1 %}
                      </div>
                    {%- endif -%}

                    <div class="multicolumn-card__info">
                      {%- if block.settings.heading_1 != blank -%}
                        <h3 class="multicolumn-card__heading">
                          {{ block.settings.heading_1 | escape }}
                        </h3>
                      {%- endif -%}
                      {%- if block.settings.text_1 != blank -%}
                        <div class="multicolumn-card__text">{{ block.settings.text_1 }}</div>
                      {%- endif -%}
                      {%- if block.settings.show_button_1 != blank -%}
                        <a
                          {% if block.settings.button_link_1 != blank %}
                            href="{{ block.settings.button_link_1 }}"
                          {% endif %}
                          class="multicolumn-card__link button button--{{ block.settings.button_style_1 }} button--{% if block.settings.button_style_1 != 'simple' %}primary-size {% else %}arrow{% endif %} focus-inset"
                          {% if block.settings.button_link == blank %}
                            aria-disabled="true"
                          {% endif %}
                          aria-label="{{ block.settings.button_label_1 | escape }}"
                        >
                          <span>
                            {{ block.settings.button_label_1 | escape }}
                          </span>
                          {% if block.settings.button_style_1 == 'simple' %}
                            {% render 'icon-button-arrow' %}
                          {% endif %}
                        </a>
                      {% endif %}
                      {%- if block.settings.additional_text_1_1 != blank
                        or block.settings.additional_text_2_1 != blank
                      -%}
                        <div class="multicolumn-card__additional-text">
                          {%- if block.settings.additional_text_1_1 != blank -%}
                            <span>{{ block.settings.additional_text_1_1 }}</span>
                          {%- endif -%}
                          {%- if block.settings.additional_text_2_1 != blank -%}
                            <span>{{ block.settings.additional_text_2_1 }}</span>
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                    </div>
                  </div>
                {% endif %}
                {% if block.settings.show_column_2 %}
                  <div class="multicolumn-card multicolumn-list__item color-{{ block.settings.color_scheme }} background">
                    {%- if block.settings.icon_2 != 'none' -%}
                      <div class="multicolumn-card__image-wrapper multicolumn-card__image-wrapper--icon multicolumn-card__image-wrapper--medium">
                        {% render 'icon-pack', icon: block.settings.icon_2 %}
                      </div>
                    {%- endif -%}

                    <div class="multicolumn-card__info">
                      {%- if block.settings.heading_2 != blank -%}
                        <h3 class="multicolumn-card__heading">
                          {{ block.settings.heading_2 | escape }}
                        </h3>
                      {%- endif -%}
                      {%- if block.settings.text_2 != blank -%}
                        <div class="multicolumn-card__text">{{ block.settings.text_2 }}</div>
                      {%- endif -%}
                      {%- if block.settings.show_button_2 != blank -%}
                        <a
                          {% if block.settings.button_link_2 != blank %}
                            href="{{ block.settings.button_link_2 }}"
                          {% endif %}
                          class="multicolumn-card__link button button--{{ block.settings.button_style_2 }} button--{% if block.settings.button_style_2 != 'simple' %}primary-size {% else %}arrow{% endif %} focus-inset"
                          {% if block.settings.button_link_2 == blank %}
                            aria-disabled="true"
                          {% endif %}
                          aria-label="{{ block.settings.button_label_2 | escape }}"
                        >
                          <span>
                            {{ block.settings.button_label_2 | escape }}
                          </span>
                          {% if block.settings.button_style_2 == 'simple' %}
                            {% render 'icon-button-arrow' %}
                          {% endif %}
                        </a>
                      {% endif %}
                      {%- if block.settings.additional_text_1_2 != blank
                        or block.settings.additional_text_2_2 != blank
                      -%}
                        <div class="multicolumn-card__additional-text">
                          {%- if block.settings.additional_text_1_2 != blank -%}
                            <span>{{ block.settings.additional_text_1_2 }}</span>
                          {%- endif -%}
                          {%- if block.settings.additional_text_2_2 != blank -%}
                            <span>{{ block.settings.additional_text_2_2 }}</span>
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                    </div>
                  </div>
                {% endif %}
                {% if block.settings.show_column_3 %}
                  <div class="multicolumn-card multicolumn-list__item color-{{ block.settings.color_scheme }} background">
                    {%- if block.settings.icon_3 != 'none' -%}
                      <div class="multicolumn-card__image-wrapper multicolumn-card__image-wrapper--icon multicolumn-card__image-wrapper--medium">
                        {% render 'icon-pack', icon: block.settings.icon_3 %}
                      </div>
                    {%- endif -%}

                    <div class="multicolumn-card__info">
                      {%- if block.settings.heading_3 != blank -%}
                        <h3 class="multicolumn-card__heading">
                          {{ block.settings.heading_3 | escape }}
                        </h3>
                      {%- endif -%}
                      {%- if block.settings.text_3 != blank -%}
                        <div class="multicolumn-card__text">{{ block.settings.text_3 }}</div>
                      {%- endif -%}
                      {%- if block.settings.show_button_3 != blank -%}
                        <a
                          {% if block.settings.button_link_3 != blank %}
                            href="{{ block.settings.button_link_3 }}"
                          {% endif %}
                          class="multicolumn-card__link button button--{{ block.settings.button_style_3 }} button--{% if block.settings.button_style_3 != 'simple' %}primary-size {% else %}arrow{% endif %} focus-inset"
                          {% if block.settings.button_link_3 == blank %}
                            aria-disabled="true"
                          {% endif %}
                          aria-label="{{ block.settings.button_label_3 | escape }}"
                        >
                          <span>
                            {{ block.settings.button_label_3 | escape }}
                          </span>
                          {% if block.settings.button_style_3 == 'simple' %}
                            {% render 'icon-button-arrow' %}
                          {% endif %}
                        </a>
                      {% endif %}
                      {%- if block.settings.additional_text_1_3 != blank
                        or block.settings.additional_text_2_3 != blank
                      -%}
                        <div class="multicolumn-card__additional-text">
                          {%- if block.settings.additional_text_1_3 != blank -%}
                            <span>{{ block.settings.additional_text_1_3 }}</span>
                          {%- endif -%}
                          {%- if block.settings.additional_text_2_3 != blank -%}
                            <span>{{ block.settings.additional_text_2_3 }}</span>
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                    </div>
                  </div>
                {% endif %}
                {% if block.settings.show_column_4 %}
                  <div class="multicolumn-card multicolumn-list__item color-{{ block.settings.color_scheme }} background">
                    {%- if block.settings.icon_4 != 'none' -%}
                      <div class="multicolumn-card__image-wrapper multicolumn-card__image-wrapper--icon multicolumn-card__image-wrapper--medium">
                        {% render 'icon-pack', icon: block.settings.icon_4 %}
                      </div>
                    {%- endif -%}

                    <div class="multicolumn-card__info">
                      {%- if block.settings.heading_4 != blank -%}
                        <h3 class="multicolumn-card__heading">
                          {{ block.settings.heading_4 | escape }}
                        </h3>
                      {%- endif -%}
                      {%- if block.settings.text_4 != blank -%}
                        <div class="multicolumn-card__text">{{ block.settings.text_4 }}</div>
                      {%- endif -%}
                      {%- if block.settings.show_button_4 != blank -%}
                        <a
                          {% if block.settings.button_link_4 != blank %}
                            href="{{ block.settings.button_link_4 }}"
                          {% endif %}
                          class="multicolumn-card__link button button--{{ block.settings.button_style_4 }} button--{% if block.settings.button_style_4 != 'simple' %}primary-size {% else %}arrow{% endif %} focus-inset"
                          {% if block.settings.button_link == blank %}
                            aria-disabled="true"
                          {% endif %}
                          aria-label="{{ block.settings.button_label_4 | escape }}"
                        >
                          <span>
                            {{ block.settings.button_label_4 | escape }}
                          </span>
                          {% if block.settings.button_style_4 == 'simple' %}
                            {% render 'icon-button-arrow' %}
                          {% endif %}
                        </a>
                      {% endif %}
                      {%- if block.settings.additional_text_1_4 != blank
                        or block.settings.additional_text_2_4 != blank
                      -%}
                        <div class="multicolumn-card__additional-text">
                          {%- if block.settings.additional_text_1_4 != blank -%}
                            <span>{{ block.settings.additional_text_1_4 }}</span>
                          {%- endif -%}
                          {%- if block.settings.additional_text_2_4 != blank -%}
                            <span>{{ block.settings.additional_text_2_4 }}</span>
                          {%- endif -%}
                        </div>
                      {%- endif -%}
                    </div>
                  </div>
                {% endif %}
              </div>
          {% endcase %}
        </div>
      {% endfor %}
    </div>
  </div>
</div>

{% schema %}
{
  "name": "t:sections.tabs-with-text.name",
  "class": "tabs-slider-section",
  "max_blocks": 7,
  "tag": "section",
  "enabled_on": {
    "templates": ["*"]
  },
  "settings": [
    {
      "type": "text",
      "id": "subheading",
      "default": "Subheading",
      "label": "t:sections.all.subheading.label"
    },
    {
      "type": "text",
      "id": "heading",
      "default": "Some heading",
      "label": "t:sections.all.heading.label"
    },
    {
      "type": "select",
      "id": "heading_size",
      "options": [
				{
          "value": "h4",
          "label": "t:sections.all.heading_size.options__4.label"
        },
        {
          "value": "h3",
          "label": "t:sections.all.heading_size.options__1.label"
        },
        {
          "value": "h2",
          "label": "t:sections.all.heading_size.options__2.label"
        },
        {
          "value": "h1",
          "label": "t:sections.all.heading_size.options__3.label"
        }
      ],
      "default": "h2",
      "label": "t:sections.all.heading_size.label"
    },
		{
      "type": "select",
      "id": "tab_heading_size",
      "options": [
				{
          "value": "h4",
          "label": "t:sections.all.heading_size.options__4.label"
        },
        {
          "value": "h3",
          "label": "t:sections.all.heading_size.options__1.label"
        }
      ],
      "default": "h3",
      "label": "t:sections.tabs-with-products.settings.tab_heading_size.label"
    },
    {
      "type": "header",
      "content": "t:sections.all.section-margin.header.content"
    },
    {
      "type": "select",
      "id": "margin_top",
      "options": [
        {
          "value": "no-indent",
          "label": "t:sections.all.section-margin.options__1.label"
        },
        {
          "value": "xs",
          "label": "t:sections.all.section-margin.options__2.label"
        },
        {
          "value": "s",
          "label": "t:sections.all.section-margin.options__3.label"
        },
        {
          "value": "m",
          "label": "t:sections.all.section-margin.options__4.label"
        },
        {
          "value": "l",
          "label": "t:sections.all.section-margin.options__5.label"
        },
        {
          "value": "xl",
          "label": "t:sections.all.section-margin.options__6.label"
        }
      ],
      "default": "no-indent",
      "label": "t:sections.all.section-margin.margin_top.label"
    },
    {
      "type": "select",
      "id": "margin_bottom",
      "options": [
        {
          "value": "no-indent",
          "label": "t:sections.all.section-margin.options__1.label"
        },
        {
          "value": "xs",
          "label": "t:sections.all.section-margin.options__2.label"
        },
        {
          "value": "s",
          "label": "t:sections.all.section-margin.options__3.label"
        },
        {
          "value": "m",
          "label": "t:sections.all.section-margin.options__4.label"
        },
        {
          "value": "l",
          "label": "t:sections.all.section-margin.options__5.label"
        },
        {
          "value": "xl",
          "label": "t:sections.all.section-margin.options__6.label"
        }
      ],
      "default": "no-indent",
      "label": "t:sections.all.section-margin.margin_bottom.label"
    },
    {
      "type": "header",
      "content": "t:sections.all.section-padding.header.content"
    },
    {
      "type": "select",
      "id": "padding_top",
      "options": [
        {
          "value": "no-indent",
          "label": "t:sections.all.section-padding.options__1.label"
        },
        {
          "value": "xs",
          "label": "t:sections.all.section-padding.options__2.label"
        },
        {
          "value": "s",
          "label": "t:sections.all.section-padding.options__3.label"
        },
        {
          "value": "m",
          "label": "t:sections.all.section-padding.options__4.label"
        },
        {
          "value": "l",
          "label": "t:sections.all.section-padding.options__5.label"
        },
        {
          "value": "xl",
          "label": "t:sections.all.section-padding.options__6.label"
        }
      ],
      "default": "m",
      "label": "t:sections.all.section-padding.padding_top.label"
    },
    {
      "type": "select",
      "id": "padding_bottom",
      "options": [
        {
          "value": "no-indent",
          "label": "t:sections.all.section-padding.options__1.label"
        },
        {
          "value": "xs",
